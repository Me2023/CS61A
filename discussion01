def is_prime(n):
    """
    >>> is_prime(10)
    False
    >>> is_prime(7)
    True
    >>> is_prime(1) # one is not a prime number!!
    False
    """
    "*** YOUR CODE HERE ***"
    x, i = 1, 0
    While x <= n:
        if n % x == 0:
            i += 1
        if i > 2:
            return False
        x += 1
    if i == 2:
        return True